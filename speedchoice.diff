+        gMain.state++;
+        break;
+    case 6:
+        PutWindowTilemap(0);
+        gMain.state++;
+        break;
+    case 7:
+        gMain.state++;
+        break;
+    case 8:
+        //PutWindowTilemap(1);
+        //DrawOptionMenuTexts();
+        gMain.state++;
+    case 9:
+        DrawDoneButtonFrame();
+        gLocalFrameTimers.totalFrames = GetDoneButtonStat(DB_FRAME_COUNT_TOTAL) + gFrameTimers.frameCount;
+        gLocalFrameTimers.totalFramesOw = GetDoneButtonStat(DB_FRAME_COUNT_OW) + gFrameTimers.owFrameCount;

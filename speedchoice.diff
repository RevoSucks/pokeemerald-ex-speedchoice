+#define TRY_INC_GAME_STAT(saveBlock, statName, max)                   \
+do {                                                                  \
+    if(gSaveBlock##saveBlock##Ptr->doneButtonStats.statName < max)    \
+        gSaveBlock##saveBlock##Ptr->doneButtonStats.statName++;       \
+}while(0)

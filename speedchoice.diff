+        PrintGameStatsPage();
+        doneButton->taskId = CreateTask(Task_DoneButtonFadeIn, 0);
+        gMain.state++;
+        break;
+    case 11:
+        BeginNormalPaletteFade(0xFFFFFFFF, 0, 16, 0, RGB_BLACK);
+        SetVBlankCallback(VBlankCB);
+        SetMainCallback2(MainCB2);
+        break;
+    }
